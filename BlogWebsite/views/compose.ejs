<%- include("partials/header") -%>

<h1>Compose</h1>

<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <input type="text" name="blogTitle" class="form-control" id="postTitle">
  </div>
  <div class="form-group">
    <label for="postText">Post</label>
    <textarea name="blogPost" rows="4" class="form-control" id="postText"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer") -%>
